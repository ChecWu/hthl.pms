  <nav class="navbar navbar-expand navbar-dark bg-dark static-top">


    <%= link_to "HTHL PMS", root_url, class: "navbar-brand mr-1" %>
    <button class="btn btn-link btn-sm text-white order-1 order-sm-0" id="sidebarToggle" href="http://www.baidu.com">
      <i class="fas fa-bars"></i>
    </button>



    <!-- Navbar -->
    <ul class="navbar-nav ml-auto ml-md-0 application-user-icon">

      <li class="nav-item dropdown no-arrow mx-1">
        <a class="nav-link dropdown-toggle" href="#" id="messagesDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
          <i class="fas fa-user-cog fa-fw application-user-icon"></i>

        </a>
        <div class="dropdown-menu dropdown-menu-right" aria-labelledby="messagesDropdown">
          <a class="dropdown-item" href="#">账户信息</a>
          <div class="dropdown-divider"></div>
          <a class="dropdown-item" href="#">• 所在部门：技术部 action</a>
          <a class="dropdown-item" href="#">• 本次登录：2019-07-03 14:36:21</a>
          <a class="dropdown-item" href="#">• 登录地区：广东省深圳市 (IP：183.14.135.1)</a>
          <a class="dropdown-item" href="#">• 上次登录：2019-07-03 14:36:21</a>
          <a class="dropdown-item" href="#" data-toggle="modal" data-target="#logoutModal">退出登录</a>
        </div>
      </li>
    </ul>
  </nav>


  <div id="wrapper">
    <!-- Sidebar -->
    <ul class="sidebar navbar-nav">
      <li class="nav-item dropdown">
        <a class="nav-link dropdown-toggle" href="#" id="pagesDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
          <i class="fas fa-fw fa-folder"></i>
          <span>首页</span>
        </a>
        <div class="dropdown-menu" aria-labelledby="pagesDropdown">
          <%= link_to "• 系统首页", root_path, class: "dropdown-item" %>
          <a class="dropdown-item" href="register.html">• 账户设置</a>
          <a class="dropdown-item" href="forgot-password.html">• 系统信息</a>
          <a class="dropdown-item" href="forgot-password.html">• 登录日志</a>
        </div>
      </li>
      <li class="nav-item dropdown">
        <a class="nav-link dropdown-toggle" href="#" id="pagesDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
          <i class="fas fa-fw fa-folder"></i>
          <span>项目管理</span>
        </a>
        <div class="dropdown-menu" aria-labelledby="pagesDropdown">
          <%= link_to "• 我的申请", approves_index_url, class: "dropdown-item" %>
