<div class="table-responsive">
<table class="table table-bordered" id="my_approve" width="100%" cellspacing="0">
  <thead>
    <tr>
      <th>编号</th>
      <th>项目名称</th>
      <th>项目编码</th>
      <th>处理环节</th>
      <th>流程状态</th>
      <th>单位名称</th>
      <th>开始时间</th>
      <th>结束时间</th>
      <th>操作</th>
    </tr>
  </thead>
  <tbody>
  <% projects.each do |project| %>
    <tr>
      <td><%= project.id %></td>
      <td><%= project.name %></td>
      <td><%= project.number %></td>
      <td><%= project.genre_name %></td>
      <td><%= project.status_name %></td>
      <td><%= project.customer.name %></td>
      <td><%= project.approved_at %></td>
      <td><%= project.done_at %></td>

      <td><%= link_to '查看', edit_user_users_url%></td>
    </tr>
   <% end %>

  </tbody>
</table>
</div>